{
    "manifest_version": 3,
    "name": "Study Timer Extension",
    "version": "1.0",
    "description": "Study timer. Study better.",
    "icons": {
        "128": "/images/icon128.png",
        "48": "/images/icon48.png",
        "16": "/images/icon16.png"
    },

    "action":{
        "default_icon": "/images/icon16.png",
        "default_popup": "index.html"
    },
    
    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]
}